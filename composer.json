{
    "name": "xpersonas/laravel-styleguide",
    "description": "Simple styleguide for use with Laravel projects. Quickly create a page of html elements and components to be styled with your front end stylesheets.",
    "keywords": ["laravel", "styleguide"],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "xpersonas",
            "email": "justin.neel@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xpersonas\\Styleguide\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xpersonas\\Styleguide\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "eloquent/composer-npm-bridge": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "timothyasp/nova-color-field":"If using Nova, consider using this color picker."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xpersonas\\Styleguide\\Providers\\StyleguideColorModelServiceProvider",
                "Xpersonas\\Styleguide\\Providers\\StyleguideServiceProvider"
            ]
        },
        "npm-bridge": {
            "timeout": 9000,
            "optional": true
        }
    }
}
